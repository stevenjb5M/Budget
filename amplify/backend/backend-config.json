{
  "api": {
    "BudgetPlannerAPI": {
      "output": {
        "authConfig": [
          {
            "additionalAuthenticationProviders": [
              {
                "apiKeyConfig": {
                  "apiKeyExpirationDays": 365,
                  "description": "API Key for testing"
                },
                "authenticationType": "API_KEY"
              }
            ],
            "defaultAuthentication": {
              "authenticationType": "AWS_IAM"
            }
          }
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "AppSync"
    }
  },
  "function": {
    "BudgetPlannerAPI": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "hosting": {
    "amplifyhosting": {
      "providerPlugin": "awscloudformation",
      "service": "amplifyhosting",
      "type": "manual"
    }
  },
  "parameters": {
    "AMPLIFY_hosting_amplifyhosting_appId": {
      "usedBy": [
        {
          "category": "hosting",
          "resourceName": "amplifyhosting"
        }
      ]
    },
    "AMPLIFY_hosting_amplifyhosting_type": {
      "usedBy": [
        {
          "category": "hosting",
          "resourceName": "amplifyhosting"
        }
      ]
    }
  },
  "storage": {
    "BudgetPlannerAssets": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB",
      "tableName": "BudgetPlanner-Assets"
    },
    "BudgetPlannerBudgets": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB",
      "tableName": "BudgetPlanner-Budgets"
    },
    "BudgetPlannerDebts": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB",
      "tableName": "BudgetPlanner-Debts"
    },
    "BudgetPlannerPlans": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB",
      "tableName": "BudgetPlanner-Plans"
    },
    "BudgetPlannerUserVersions": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB",
      "tableName": "BudgetPlanner-UserVersions"
    },
    "BudgetPlannerUsers": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB",
      "tableName": "BudgetPlanner-Users"
    }
  }
}